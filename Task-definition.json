{
    "family": "sample-fargate",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "my-container",
        "image": "871909687521.dkr.ecr.us-east-1.amazonaws.com/action-ci-cd:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "http"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  